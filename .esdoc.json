{
  "source": "./src",
  "destination": "./docs",
  "importPathReplace": {
    "from": "/.js",
    "to": "/index.js"
  },
  "plugins": [
    {"name": "esdoc-jsx-plugin"},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {
      "classProperties": true,
      "objectRestSpread": true,
      "functionBind": true,
      "exportExtensions": true
    }},
    {"name": "esdoc-standard-plugin"}
  ]
}

